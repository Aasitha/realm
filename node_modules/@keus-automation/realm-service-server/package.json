  "scripts": {
    "build": "rmb --input './model/*-collection.ts' --outdir './src/' && tsc",
    "prepublish": "npm run build",
    "babel-examples": "babel -w --extensions .ts example/ --out-dir example-dist/",
    "build-src-model": "rmb --input './model/*-collection.ts' --outdir './src/' --watch true",
    "tsc-watch": "tsc -w",
    "watch": "concurrently 'npm run build-src-model' 'npm run tsc-watch' 'npm run babel-examples'"
  }
